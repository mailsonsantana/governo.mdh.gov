<?xml version="1.0"?>
<object name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="governo.mdh.portal"
    purge="False">
  <configlet title="Arquivos Corrompidos"
      action_id="broken"
      appId="governo.mdh.portal"
      category="Products"
      condition_expr=""
      url_expr="string:${portal_url}/@@broken-files"
      icon_expr="string:${portal_url}/++resource++portalmdh/img/file_broken.png"
      visible="True"
      i18n:attributes="title">
    <permission>governo.mdh.portal: Setup</permission>
  </configlet>
</object>
