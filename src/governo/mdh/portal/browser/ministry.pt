<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
    xmlns:tal="http://xml.zope.org/namespaces/tal"
    xmlns:metal="http://xml.zope.org/namespaces/metal"
    metal:use-macro="context/main_template/macros/master"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="governo.mdh.portal">
  <body>
    <metal:main fill-slot="main">
      <article>
        <div tal:replace="structure provider:plone.abovecontenttitle" />

        <div  class="linha-destaquetopo">

          <div id="parent-fieldname-lead_image">
            <img
                tal:define="scales context/@@images;
                            scale python: scales.scale('lead_image', scale=None)"
                tal:replace="structure scale/tag|nothing" />
          </div>
          <h1 class="documentFirstHeading"
              tal:content="context/title">Ministry</h1>

          <div class="documentDescription"
              tal:content="context/description">Description</div>

        </div>

        <!-- <div i18n:translate="label_minister" class="label-minister">Minister</div>
        <h2 id="parent-fieldname-minister"
            tal:content="context/minister">Name</h2> -->

        <div tal:replace="structure provider:plone.belowcontenttitle" />

        <div tal:replace="structure provider:plone.abovecontentbody" />

        <div id="content-core"
            tal:define="is_editable context/@@plone_context_state/is_editable;
                        text context/text/output|nothing">

          <p class="discreet"
              tal:condition="python:not text and is_editable"
              i18n:domain="plone"
              i18n:translate="no_body_text">
            This item does not have any body text, click the edit tab to change it.
          </p>

          <div id="parent-fieldname-text" tal:condition="text">
            <div tal:content="structure text">The body</div>
          </div>
        </div>

        <div tal:replace="structure provider:plone.belowcontentbody" />
      </article>

      <tal:agenda condition="view/has_agenda">
        <div class="dados-agenda" data-url="http://165.227.47.203/acompanhe-planalto/agenda-do-presidente/agenda-do-presidente-michel-temer">
        
        <div class="daypicker-wrapper">
          <ul class="daypicker"><li data-day="2018-04-09T03:00:00.000Z" class="day">
          <div class="daypicker-day">9</div>
          <div class="daypicker-weekday">Seg</div>
        </li><li data-day="2018-04-10T03:00:00.000Z" class="day">
          <div class="daypicker-day">10</div>
          <div class="daypicker-weekday">Ter</div>
        </li><li data-day="2018-04-11T03:00:00.000Z" class="day">
          <div class="daypicker-day">11</div>
          <div class="daypicker-weekday">Qua</div>
        </li><li data-day="2018-04-12T03:00:00.000Z" class="day is-selected">
          <div class="daypicker-day">12</div>
          <div class="daypicker-weekday">Qui</div>
        </li><li data-day="2018-04-13T03:00:00.000Z" class="day">
          <div class="daypicker-day">13</div>
          <div class="daypicker-weekday">Sex</div>
        </li><li data-day="2018-04-14T03:00:00.000Z" class="day">
          <div class="daypicker-day">14</div>
          <div class="daypicker-weekday">SÃ¡b</div>
        </li><li data-day="2018-04-15T03:00:00.000Z" class="day">
          <div class="daypicker-day">15</div>
          <div class="daypicker-weekday">Dom</div>
        </li></ul>
        </div>
        <div class="collectionevents-title-wrapper">
          <div class="collectionevents-stroke"></div>
          <div class="collectionevents-title">Compromissos do dia</div>
          <div class="collectionevents-stroke"></div>
        </div>
      </div>
      </tal:agenda>
     </metal:main>
  </body>
</html>